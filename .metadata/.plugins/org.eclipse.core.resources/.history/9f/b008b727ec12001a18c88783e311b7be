package prjGUITestScores_KadenPatton;

public class Student {
	
	private String name;
	private int[] tests;
	
	public Student()
	{
		this("");
	}
	
	public Student(String nm) {
		this(name, 3);
	}
	
	
	public Student(String nm, int n)
	{
		name = n;
		tests = new int[n];
		for(int = 0; i < tests.length; i++)
		{
			test[i] = 0;
		}
	}
	
}
