package prjGUITestScores_KadenPatton;

public class Student {
	
	private String name;
	private int[] tests;
	
	public Student()
	{
		this("");
	}
	
	public Student(String nm) {
		this(nm, 3);
	}
	
	
	public Student(String nm, int n)
	{
		name = n;
		tests = new int[n];
		for(int i = 0; i < tests.length; i++)
		{
			tests[i] = 0;
		}
	}
	
}
